#: ../lib/rb-cut-and-paste-code.c:105
msgid "Today %I:%M %p"
msgstr "Днес, %H:%M"

#. Translators: "friendly time" string for the previous day,
#. * strftime format. e.g. "Yesterday 12:34 am"
#.
#: ../lib/rb-cut-and-paste-code.c:116
msgid "Yesterday %I:%M %p"
msgstr "Вчера, %H:%M"

